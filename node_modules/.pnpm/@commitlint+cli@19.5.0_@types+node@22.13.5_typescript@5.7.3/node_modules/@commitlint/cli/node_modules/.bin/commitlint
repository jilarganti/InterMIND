#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kingwww/VSCode/GoldenFishProject/node_modules/.pnpm/@commitlint+cli@19.5.0_@types+node@22.13.5_typescript@5.7.3/node_modules/@commitlint/cli/node_modules:/Users/kingwww/VSCode/GoldenFishProject/node_modules/.pnpm/@commitlint+cli@19.5.0_@types+node@22.13.5_typescript@5.7.3/node_modules/@commitlint/node_modules:/Users/kingwww/VSCode/GoldenFishProject/node_modules/.pnpm/@commitlint+cli@19.5.0_@types+node@22.13.5_typescript@5.7.3/node_modules:/Users/kingwww/VSCode/GoldenFishProject/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kingwww/VSCode/GoldenFishProject/node_modules/.pnpm/@commitlint+cli@19.5.0_@types+node@22.13.5_typescript@5.7.3/node_modules/@commitlint/cli/node_modules:/Users/kingwww/VSCode/GoldenFishProject/node_modules/.pnpm/@commitlint+cli@19.5.0_@types+node@22.13.5_typescript@5.7.3/node_modules/@commitlint/node_modules:/Users/kingwww/VSCode/GoldenFishProject/node_modules/.pnpm/@commitlint+cli@19.5.0_@types+node@22.13.5_typescript@5.7.3/node_modules:/Users/kingwww/VSCode/GoldenFishProject/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
