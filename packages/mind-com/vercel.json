{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,

  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/company/about/",
      "destination": "/resources/company/about/",
      "permanent": true
    }
  ]
}
