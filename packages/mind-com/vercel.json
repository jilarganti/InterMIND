{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,

  "redirects": [
    {
      "source": "/promo/imind",
      "destination": "/imind",
      "permanent": true
    }
  ],

  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800, immutable"
        }
      ]
    }
  ]
}
