{
  "framework": "vitepress",
  "buildCommand": "pnpm run build",
  "outputDirectory": ".vitepress/dist",
  "routes": [
    {
      "src": "/api/search-images",
      "headers": {
        "cache-control": "public, max-age=86400, s-maxage=604800"
      }
    }
  ],
  "functions": {
    "api/chat.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
